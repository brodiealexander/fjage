{
  "name": "fjage",
  "version": "1.9.0",
  "description": "Framework for Java and Groovy Agents",
  "exports": "./dist/cjs/fjage.cjs",
  "module": "dist/mjs/fjage.js",
  "files": [
    "dist/**"
  ],
  "private": "true",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm version --allow-same-version $(cat '../../VERSION'); rm -rf dist/ && rollup -c rollup.config.js",
    "test": "rm test/spec/fjageSpec.* && node test/spec/create-spec.cjs && jasmine test/spec/fjageSpec.cjs; node test/browser-test.cjs",
    "clean": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/org-arl/fjage.git"
  },
  "contributors": [
    "Mandar Chitre (http://www.chitre.net)",
    "Chinmay Pendharkar (http://chinpen.net)"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/org-arl/fjage/issues"
  },
  "homepage": "https://github.com/org-arl/fjage#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "documentation": "^13.2.5",
    "jasmine": "^3.7.0",
    "node-static": "^0.7.11",
    "puppeteer": "^10.0.0",
    "rollup": "^2.51.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "browser-or-node": "^1.3.0"
  }
}
