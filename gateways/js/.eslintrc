{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "indent": ["error", 2],
    "quotes":["error", "single"],
    "semi": ["error", "always"]
  }
}
